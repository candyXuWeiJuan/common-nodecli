{
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "all",
	"jsxBracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "crlf",
	"embeddedLanguageFormatting": "auto",
	"pugBracketSpacing": true,
	"pugSingleQuote": false
}
